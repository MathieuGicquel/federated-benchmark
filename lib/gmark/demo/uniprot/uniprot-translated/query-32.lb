BEGIN
gmarkSubquery00(x, y) <- core:edge:edge(x1, 1, x0), core:edge:edge(x1, 0, x2), core:edge:edge(x2, 0, x3), core:edge:edge(x4, 0, x3), x = x0, y = x4.
gmarkSubquery00(x, y) <- core:edge:edge(x1, 1, x0), core:edge:edge(x1, 0, x2), core:edge:edge(x3, 0, x2), core:edge:edge(x3, 0, x4), x = x0, y = x4.
gmarkSubquery00(x, y) <- core:edge:edge(x1, 1, x0), core:edge:edge(x2, 0, x1), x = x0, y = x2.
gmarkSubquery01(x, y) <- core:edge:edge(x0, 0, x1), core:edge:edge(x1, 0, x2), x = x0, y = x2.
gmarkSubquery01(x, x) <- core:edge:edge(x, r, y).
gmarkSubquery01(y, y) <- core:edge:edge(x, r, y).
gmarkSubquery01(x, y) <- gmarkSubquery01(x, z), gmarkSubquery01(z, y).
gmarkSubquery02(x, y) <- core:edge:edge(x0, 0, x1), core:edge:edge(x1, 0, x2), core:edge:edge(x3, 0, x2), core:edge:edge(x3, 0, x4), x = x0, y = x4.
gmarkSubquery02(x, x) <- core:edge:edge(x, r, y).
gmarkSubquery02(y, y) <- core:edge:edge(x, r, y).
gmarkSubquery02(x, y) <- gmarkSubquery02(x, z), gmarkSubquery02(z, y).
query(x3, x2, x1, x0) <- gmarkSubquery00(x0, x1), gmarkSubquery01(x1, x2), gmarkSubquery02(x2, x3).
END
