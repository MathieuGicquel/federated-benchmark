PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>

CONSTRUCT {
    GRAPH ?g {
         ?country_translated  rdf:type ?typeURI .
     }
} WHERE { 
 ?s (<http://example.org/_/parentCountry>|<http://example.org/_/nationality>) ?country .
 
 BIND(URI(CONCAT("http://example.org/", "federated_shop")) AS ?g) .
 BIND(URI(REPLACE(STR(?country), "_/", "federated_shop/")) AS ?country_translated) .

 BIND(REPLACE(STR(?country), STR("http://example.org/_/"),"") AS ?type1) .
 BIND(REPLACE(STR(?type1), STR("_[0-9]+"),"") AS ?type) .
 BIND(URI(CONCAT("http://example.org/federated_shop/", ?type)) AS ?typeURI) .

}